{
  "type": "module",
  "name": "legacy-of-umah",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "tsc --noEmit && eslint \"src/**/*.{ts,tsx}\" --cache",
    "format": "prettier --cache src/**/*.{ts,tsx} --write --cache",
    "tree": "pnpm tsc --target ESNext --module ESNext --esModuleInterop --allowJs --skipLibCheck --downlevelIteration ./src/structure/structure.ts && node ./src/structure/structure.js"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": "22"
  },
  "dependencies": {
    "next": "15.1.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/eslint": "^9.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9",
    "eslint-config-next": "15.1.2",
    "eslint-plugin-prefer-arrow-functions": "^3.4.1",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
